{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2017"
    ],
    "allowJs": false,
    "checkJs": false,
    "jsx": "react",
    "declaration": false,
    "sourceMap": true,
    "rootDir": "./",
    "removeComments": false,
    "noEmit": false,
    "importHelpers": true,
    "downlevelIteration": true,
    "isolatedModules": false,


    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,

    "moduleResolution": "node",
    "baseUrl": "./",
    "sourceRoot": "./src"
  }
}
